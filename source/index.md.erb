<div class="frontpage-container">
    <section role="frontpage" class="dashboard fadeOutBack">
        <!-- frontpage-grid -->
        <div id="metro-container" class="metro-container">

            <div class="metro-row">
                <div id="logo" class="metro-tile logo">
                   <%= image_tag "thinkgeneric_logo.svg" %>
                    
                </div>
            </div>

            <div class="metro-row">
                <div class="metro-tile w4 void">
                    <div class="metro-tile-content slogan">
                        <h2 class="tagline">Website Design and Application Development</h2>
                        <p>We are a St Petersburg based interactive web design firm specializing in functional, yet concise design.  Using online marketing, web and graphic design we will work with you to convert your visitors into clients.</p>
                    </div>
                </div>
                <div class="metro-tile mobile">
                    <div class="metro-tile-content img">
                        <div class="apsect-box">
                        <%= image_tag "header.png" %>
                        </div>
                    </div>
                </div>
                <div class="metro-tile mobile">
                    <div class="metro-tile-content">
                        <h1 class="tagline hp-center">Website Design and Application Development</h1>
