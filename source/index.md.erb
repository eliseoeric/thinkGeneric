                </div>

                <div class="metro-tile overlay">

                    <a href="<%=current_page.data.fourth_link%>" class="nav odd"><div class="nav-overlay"></div><%=current_page.data.fourth_link_icon%><%=current_page.data.fourth_link_title%></a>
                </div>

                <div class="metro-tile">
                    <div class="metro-tile-content img">
                        <div class="apsect-box">
                        <%= image_tag "workspace_800.png" %>
                        </div>                    
