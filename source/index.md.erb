                    </div>
                </div>
                <div class="metro-tile overlay">

                    <a href="<%=current_page.data.first_link %>" class="nav even"><div class="nav-overlay"></div><%=current_page.data.first_link_icon %><%=current_page.data.first_link_title%></a>
                </div>

                <div class="metro-tile ">
                    <div class="metro-tile-content img">
                        <div class="apsect-box">
                       
                         <%= image_tag "photoshop_800.png" %>
                        </div>
                    </div>
                </div>
            </div><!--end row -->


            <div class="metro-row">
                <div class="metro-tile overlay">

                    <a href="<%=current_page.data.second_link%>" class="nav odd"><div class="nav-overlay"></div><%=current_page.data.second_link_icon%><%=current_page.data.second_link_title%></a>
